model BankAccount {
  id           String   @id @default(uuid())
  bank_code    Int
  bank_name    String
  account_name String
  agency       String
  account      String
  check_digit  String
  created_at   DateTime @default(now())
  updated_at   DateTime @updatedAt

  user    User   @relation(fields: [user_id], references: [id])
  user_id String

  @@map("bank_accounts")
}

model User {
  id            String        @id
  bank_accounts BankAccount[]
}
